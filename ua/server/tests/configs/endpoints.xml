<?xml version="1.0"?>
<config>
  <modules>

    <module>
      <id>tcp_server</id>
      <path>./.libs/libbuiltin_computer_addon.so</path>
    </module>

    <module>
      <id>internal_computer</id>
      <path>./.libs/libinternal_computer_addon.so</path>
    </module>

    <module>
      <id>endpoints_services</id>
      <path>./.libs/libendpoints_services_addon.so</path>
      <depends_on>
        <id>internal_computer</id>
      </depends_on>
    </module>

    <module>
      <id>view_services</id>
      <path>./.libs/libview_services_addon.so</path>
      <depends_on>
        <id>internal_computer</id>
      </depends_on>
    </module>


    <module>
      <id>opcua_protocol</id>
      <path>./.libs/libopcua_protocol.so</path>
      <depends_on>
        <id>tcp_server</id>
        <id>internal_computer</id>
        <id>endpoints_services</id>
      </depends_on>
      <parameters>
        <application_name>Test OPC UA Server</application_name>
        <application_uri>opcua.treww.org</application_uri>
        <application_type>server</application_type>
        <security_mode>none</security_mode>
        <transport_profile_uri>http://opcfoundation.org/UA-Profile/Transport/uatcp-uasc-uabinary</transport_profile_uri>
        <user_token_policy>
          <id>anonymous</id>
          <type>anonymous</type>
          <uri>http://opcfoundation.org/UA/SecurityPolicy#None</uri>
        </user_token_policy>
        <endpoint>
          <url>opc.tcp://localhost:4841</url>
        </endpoint>
      </parameters>
    </module>

  </modules>
</config>

